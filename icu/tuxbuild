pkgname=icu
pkgver=77.1
pkgurls=(https://github.com/unicode-org/icu/releases/download/release-77-1/icu4c-77_1-src.tgz)
depends=()

buildpkg() {
    tar xf icu4c-77_1-src.tgz
    cd icu/source
    ./configure --prefix=/usr
    make || make -j1
}

installpkg() [
    cd icu/source
    make DESTDIR=$DST install
]