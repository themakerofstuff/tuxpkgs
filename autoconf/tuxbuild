pkgname=autoconf
pkgver=2.72
pkgurls=(https://ftp.gnu.org/gnu/autoconf/autoconf-$pkgver.tar.xz)
depends=()

buildpkg() {
    tar xf $pkgname-$pkgver.tar.xz
    cd $pkgname-$pkgver
    ./configure --prefix=/usr
    make || make -j1
}

installpkg() {
    cd $pkgname-$pkgver
    make DESTDIR=$DST install
}