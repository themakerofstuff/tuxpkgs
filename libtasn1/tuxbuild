pkgname=libtasn1
pkgver=4.19.0
pkgurls=(https://ftp.gnu.org/gnu/libtasn1/libtasn1-$pkgver.tar.gz)
depends=()

buildpkg() {
    tar xf $pkgname-$pkgver.tar.gz
    cd $pkgname-$pkgver
    ./configure --prefix=/usr --disable-static
    make || make -j1
}

installpkg() {
    cd $pkgname-$pkgver
    make DESTDIR=$DST install
}