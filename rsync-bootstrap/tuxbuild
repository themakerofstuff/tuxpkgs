pkgname=rsync-bootstrap
pkgver=3.3.0
pkgurls=(https://samba.org/ftp/rsync/src/rsync-$pkgver.tar.gz)
depends=(popt-bootstrap)

buildpkg() {
	tar xf rsync-$pkgver.tar.gz
	cd rsync-$pkgver
	./configure --prefix=/usr --host=$CC_TGT --disable-xxhash
	make || make -j1
}

installpkg() {
	cd rsync-$pkgver
	make DESTDIR=$DST install
}
